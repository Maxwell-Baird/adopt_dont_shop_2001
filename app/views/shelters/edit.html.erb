<%= form_tag "/shelters/#{@shelter.id}", method: :patch do %>
  <h3> Edit shelter </h3>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  <p></p>
  <%= label_tag :address %>
  <%= text_field_tag :address %>
  <p></p>
  <%= label_tag :city %>
  <%= text_field_tag :city %>
  <p></p>
  <%= label_tag :state %>
  <%= text_field_tag :state %>
  <p></p>
  <%= label_tag :zip %>
  <%= text_field_tag :zip %>
  <p></p>
  <%= submit_tag 'submit' %>
<% end %>
